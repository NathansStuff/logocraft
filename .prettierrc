{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,

    "printWidth": 120,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "parser": "typescript",
    "endOfLine": "lf",
    "singleAttributePerLine": true,
    "jsxSingleQuote": true,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}
